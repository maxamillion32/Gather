<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sweettooth.cs.brandeis.edu.eventsapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DatabaseTester">
            android:label="Database Tester">
            <intent-filter>
                <action android:name="sweettooth.cs.brandeis.edu.eventsapp.DatabaseTester" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>



        <activity android:name=".GoogleAuth">
            android:label="Database Tester">
            <intent-filter>
                <action android:name="sweettooth.cs.brandeis.edu.eventsapp.GoogleAuth" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


    </application>

</manifest>